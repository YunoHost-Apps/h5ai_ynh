{
    "name": "h5ai",
    "id": "h5ai",
    "packaging_format": 1,
    "description": {
        "en": "Modern HTTP web server index for NGINX",
        "fr": "Serveur moderne d'index pour NGINX"
    },
    "version": "0.30.0~ynh2",
    "url": "https://larsjung.de/h5ai/",
    "upstream": {
        "license": "MIT",
        "website": "https://larsjung.de/h5ai/",
        "admindoc": "https://larsjung.de/h5ai/",
        "code": "https://github.com/lrsjng/h5ai"
    },
    "license": "MIT",
    "maintainer": {
        "name": "alexAubin",
        "email": "alex.aubin@mailoo.org"
    },
    "requirements": {
        "yunohost": ">= 4.2.0"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/documents",
                "default": "/documents"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
